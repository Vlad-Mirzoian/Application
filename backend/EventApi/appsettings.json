{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=db;Port=5432;Database=eventdb;Username=postgres;Password=${DB_PASS}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "EventApi",
    "Audience": "EventApiClient"
  }
}